    //Insertar cursor: editor.action.insertCursorBelow
        //Linux
        {
            "key": "ctrl+shift+down",
            "command": "-editor.action.insertCursorBelow",
            "when": "editorTextFocus && isLinux" 
        },
            {
                "key": "ctrl+shift+down",
                "command": "editor.action.insertCursorBelow",
                "when": "editorTextFocus && isLinux" 
            },
        //Windows
        {
            "key": "ctrl+alt+down",
            "command": "-editor.action.insertCursorBelow",
            "when": "editorTextFocus && isWindows" 
        },
